{
  "name": "uswds-starter-site",
  "version": "1.0.0",
  "description": "A minimal 11ty starter site with USWDS and Netlify CMS",
  "scripts": {
    "start": "eleventy --serve",
    "build": "eleventy",
    "clean": "rm -rf _site",
    "cms:proxy": "npx netlify-cms-proxy-server",
    "dev": "npm-run-all --parallel start cms:proxy"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": ["11ty", "uswds", "netlify-cms", "eleventy", "static-site"],
  "author": "",
  "license": "CC0-1.0",
  "devDependencies": {
    "netlify-cms-proxy-server": "^1.3.24",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@uswds/uswds": "^3.10.0",
    "@11ty/eleventy": "^3.0.0"
  }
}
